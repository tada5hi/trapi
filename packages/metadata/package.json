{
    "name": "@trapi/metadata",
    "version": "1.0.0-alpha.2",
    "description": "Generate REST-API metadata scheme from TypeScript Decorators.",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "start": "tsc -p tsconfig.build.json -w",
        "build": "npm run clean && tsc -p tsconfig.build.json",
        "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "npm run test -- --coverage",
        "preblushOnly": "npm run build",
        "generate": "node ./bin/generate.js"
    },
    "keywords": [
        "typescript",
        "REST-API",
        "decorators",
        "metadata",
        "types",
        "analyze",
        "generation",
        "decorator"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tada5hi/trapi.git",
        "directory": "packages/metadata"
    },
    "bugs": {
        "url": "https://github.com/Tada5hi/trapi/issues"
    },
    "homepage": "https://github.com/Tada5hi/trapi#readme",
    "devDependencies": {
        "jsonata": "^2.0.2",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "locter": "^1.0.9",
        "minimatch": "^7.4.2"
    },
    "peerDependencies": {
        "typescript": "4.7.x || 4.8.x || 4.9.x"
    },
